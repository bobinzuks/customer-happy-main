{
  "name": "gsurveyai",
  "version": "1.0.0",
  "description": "AI-Powered Customer Survey & Review Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run db:generate && tsc --skipLibCheck",
    "start": "npm run db:generate && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t gsurveyai .",
    "docker:run": "docker run -p 3000:3000 gsurveyai",
    "deploy": "./cloudflare-deploy.sh"
  },
  "keywords": [
    "saas",
    "customer-experience",
    "ai",
    "reviews",
    "interview",
    "sentiment-analysis",
    "business-analytics"
  ],
  "author": "GSurveyAI Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/static": "^8.0.1",
    "@fastify/websocket": "^11.0.1",
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "fastify": "^5.3.0",
    "groq-sdk": "^0.7.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.5.0",
    "prisma": "^6.9.0",
    "redis": "^5.5.6",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.29.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}